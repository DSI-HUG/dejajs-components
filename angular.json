{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "dejajs-component": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss",
                    "flat": false,
                    "changeDetection": "OnPush"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/demo",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            {
                                "glob": "404.html",
                                "input": "src",
                                "output": "/"
                            },
                            {
                                "glob": "**/*",
                                "input": "node_modules/monaco-editor/min",
                                "output": "assets/monaco/"
                            },
                            {
                                "glob": "**/*",
                                "input": "node_modules/ckeditor/",
                                "output": "assets/ckeditor/"
                            }
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "allowedCommonJsDependencies": [
                            "showdown",
                            "ckeditor"
                        ],
                        "scripts": [],
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "baseHref": "https://dsi-hug.github.io/dejajs-components/",
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "dejajs-component:build",
                        "port": 5100
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "dejajs-component:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "dejajs-component:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["src/**/*.js", "src/**/*.ts", "src/**/*.component.html"]
                    }
                }
            }
        },
        "@deja-js/component": {
            "root": "projects/deja-js/component",
            "sourceRoot": "projects/deja-js/component",
            "projectType": "library",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/deja-js/component/tsconfig.lib.json",
                        "project": "projects/deja-js/component/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/deja-js/component/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/deja-js/component/test.ts",
                        "tsConfig": "projects/deja-js/component/tsconfig.spec.json",
                        "karmaConfig": "projects/deja-js/component/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["projects/deja-js/component/**/*.js", "projects/deja-js/component/**/*.ts", "projects/deja-js/component/**/*.component.html"]
                    }
                }
            }
        }
    },
    "cli": {
      "analytics": "7efcc763-d0c9-4deb-b139-349e9d721ec1"
    }
}
