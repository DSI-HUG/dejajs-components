{
    "root": true,
    "extends": [
        "@hug/eslint-config/recommended"
    ],
    "overrides": [{
        "files": ["src/**/*.ts"],
        "plugins": ["deprecation"],
        "rules": {
            "@angular-eslint/prefer-on-push-component-change-detection": "off",
            "deprecation/deprecation": "warn"
        }
    }]
}
