{
    "name": "dejajs-component",
    "version": "10.0.0-beta.2S",
    "description": "Angular Components Library",
    "homepage": "https://github.com/DSI-HUG/dejajs-components",
    "bugs": "https://github.com/DSI-HUG/dejajs-components/issues",
    "author": "Team DSI-HUG - Hôpital Univesitaire de Genève - DSI - Domaine Patient",
    "license": "Apache-2.0",
    "keywords": [
        "angular",
        "material",
        "component"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DSI-HUG/dejajs-components.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "test:core": "ng test @deja-js/core --watch=false",
        "test:lib": "ng test @deja-js/component --watch=false",
        "test:lib:watch": "ng test @deja-js/component --watch=true",
        "lint": "eslint . --ext .js,.ts",
        "e2e": "ng e2e",
        "license-check": "node scripts/licenses.js",
        "build:core": "ng build @deja-js/core --configuration=production",
        "build:lib": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build @deja-js/component --configuration=production && npm run build:scss",
        "build:lib:watch": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build @deja-js/component --watch && npm run build:scss",
        "build:scss": "scss-bundle -c component-scss-bundle.config.json",
        "changelog:major": "changelog -M && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\"",
        "changelog:minor": "changelog -m && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\"",
        "changelog:patch": "changelog -p && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\"",
        "release:lib:major": "npm run changelog:major && cd projects/deja-js/component && npm version major && git push origin",
        "release:lib:minor": "npm run changelog:minor && cd projects/deja-js/component && npm version minor && git push origin",
        "release:lib:patch": "npm run changelog:patch && cd projects/deja-js/component && npm version patch && git push origin",
        "release:core:major": "npm run changelog:major && cd projects/deja-js/core && npm version major && git push origin",
        "release:core:minor": "npm run changelog:minor && cd projects/deja-js/core && npm version minor && git push origin",
        "release:core:patch": "npm run changelog:patch && cd projects/deja-js/core && npm version patch && git push origin",
        "postinstall": "ngcc"
    },
    "dependencies": {
        "@angular/animations": "~10.1.5",
        "@angular/cdk": "~10.2.4",
        "@angular/common": "~10.1.5",
        "@angular/compiler": "~10.1.5",
        "@angular/core": "~10.1.5",
        "@angular/forms": "~10.1.5",
        "@angular/localize": "~10.1.5",
        "@angular/material": "~10.2.4",
        "@angular/material-moment-adapter": "~10.2.4",
        "@angular/platform-browser": "~10.1.5",
        "@angular/platform-browser-dynamic": "~10.1.5",
        "@angular/router": "~10.1.5",
        "angular2-text-mask": "9.0.0",
        "ckeditor": "~4.11.0",
        "core-js": "~3.6.5",
        "hammerjs": "~2.0.8",
        "json-object-mapper": "~1.6.0",
        "material-icons-font": "2.1.0",
        "moment": "2.29.1",
        "monaco-editor": "0.21.2",
        "prismjs": "~1.22.0",
        "rxjs": "~6.6.3",
        "showdown": "~1.9.1",
        "tslib": "~2.0.3",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@types/jasmine": "~3.5.14",
        "@types/jasminewd2": "~2.0.8",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~6.0.0",
        "karma": "~5.2.3",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "~1.5.4",
        "@angular-devkit/build-angular": "~0.1001.6",
        "@angular-devkit/build-ng-packagr": "~0.1001.6",
        "@angular/cli": "~10.1.6",
        "@angular/compiler-cli": "~10.1.5",
        "@angular/language-service": "~10.1.5",
        "@angular-eslint/builder": "^0.5.0-beta.3",
        "@angular-eslint/eslint-plugin": "^0.5.0-beta.3",
        "@angular-eslint/eslint-plugin-template": "^0.5.0-beta.3",
        "@angular-eslint/template-parser": "^0.5.0-beta.3",
        "@types/lodash": "~4.14.162",
        "@types/node": "~14.11.8",
        "@types/prismjs": "~1.16.1",
        "@types/showdown": "~1.9.3",
        "@types/ckeditor": "~4.9.10",
        "@typescript-eslint/eslint-plugin": "~4.4.0",
        "@typescript-eslint/parser": "~4.4.0",
        "codelyzer": "~6.0.1",
        "eslint": "^7.11.0",
        "eslint-plugin-chai-friendly": "^0.6.0",
        "eslint-plugin-cypress": "^2.11.2",
        "eslint-plugin-no-loops": "^0.3.0",
        "eslint-plugin-prefer-arrow": "^1.2.2",
        "eslint-plugin-rxjs": "^0.0.3-beta.30",
        "eslint-plugin-rxjs-angular": "^0.0.2-beta.6",
        "eslint-plugin-simple-import-sort": "^5.0.3",
        "cz-conventional-changelog": "~3.3.0",
        "gh-pages": "~3.1.0",
        "ng-packagr": "~10.1.2",
        "protractor": "~7.0.0",
        "scss-bundle": "~3.1.2",
        "ts-node": "~9.0.0",
        "typescript": "~4.0.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
