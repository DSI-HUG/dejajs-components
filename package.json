{
  "name": "@deja-js/component",
  "version": "1.8.1",
  "description": "Angular2 Components Library",
  "homepage": "https://github.com/DSI-HUG/dejajs-components",
  "bugs": "https://github.com/DSI-HUG/dejajs-components/issues",
  "author": "Team DSI-HUG - Hôpital Univesitaire de Genève - DSI - Domaine Patient",
  "license": "Apache-2.0",
  "keywords": [
    "angular2",
    "material",
    "component",
    "grid",
    "datagrid",
    "data-grid",
    "datepicker",
    "colorpicker",
    "messagebox",
    "monaco",
    "select",
    "textarea",
    "tiles",
    "snackbar",
    "demo",
    "demo-app",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DSI-HUG/dejajs-components.git"
  },
  "scripts": {
    "build-css": "node-sass src/scss/index.scss -o ./src/scss --importer=node_modules/node-sass-tilde-importer",
    "build": "webpack-dev-server --inline --hot --progress --port 5100 --config webpack.config.dev.js",
    "start": "npm run build-css && npm run build",
    "prepublish": "npm run build-css",
    "build:prod": "del-cli dist && npm run build-css && webpack -p --config webpack.config.prod.js && copy 404.html dist",
    "e2e": "webdriver-manager update && protractor specs/protractor.conf.js",
    "test": "npm run e2e",
    "licensecheck": "license-check",
    "changelog": "ez-changelog"
  },
  "dependencies": {
    "@angular/animations": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/router": "^4.1.0",
    "@angular/upgrade": "^4.1.0",
    "@deja-js/resources": "^1.1.4",
    "@types/chai": "^3.5.2",
    "@types/core-js": "^0.9.41",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.15",
    "@types/prismjs": "^1.6.2",
    "@types/showdown": "^1.4.32",
    "angular2-text-mask": "^8.0.1",
    "bluebird": "^3.5.0",
    "codelyzer": "^3.0.1",
    "core-js": "^2.4.1",
    "custom-event-polyfill": "^0.3.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.18.1",
    "monaco-editor": "^0.8.3",
    "prismjs": "^1.6.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.3.1",
    "showdown": "^1.6.4",
    "x2js": "^3.1.1",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "concurrently": "^3.4.0",
    "copy": "^0.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "cz-conventional-changelog": "^2.0.0",
    "del-cli": "^0.2.1",
    "exports-loader": "^0.6.4",
    "ez-changelog": "^3.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "license-check": "^1.1.5",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "protractor": "^5.1.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "tslint": "^5.1.0",
    "typedoc": "^0.6.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^2.3.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-cyclic-dependency-checker": "^0.0.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "license-check-config": {
    "src": [
      "**/*.ts",
      "!./node_modules/**/*",
      "!**/*.d.ts"
    ],
    "path": "header-license.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
