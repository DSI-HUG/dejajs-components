{
  "name": "@deja-js/component",
  "version": "1.4.0",
  "description": "Angular2 Components Library",
  "homepage": "https://github.com/DSI-HUG/deja-js",
  "bugs": "https://github.com/DSI-HUG/deja-js/issues",
  "author": "Team DSI-HUG - Hôpital Univesitaire de Genève - DSI - Domaine Patient",
  "license": "Apache-2.0",
  "keywords": [
    "angular2",
    "material",
    "component",
    "grid",
    "datagrid",
    "data-grid",
    "datepicker",
    "colorpicker",
    "messagebox",
    "monaco",
    "select",
    "textarea",
    "tiles",
    "snackbar",
    "demo",
    "demo-app",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DSI-HUG/dejajs-components.git"
  },
  "scripts": {
    "drug": "json-server --port 5102 --ro -q ./src/demo-app/services/drug-event-0003-of-0003-2.json",
    "countries": "json-server --port 5101 --ro -q ./src/demo-app/services/countries.json",
    "build-css": "node-sass src/scss/index.scss -o ./src/scss",
    "build": "webpack-dev-server -d --devtool source-map --inline --progress --port 5100 --config webpack.config.dev.js",
    "start": " npm run build-css && concurrently \"npm run countries\" \"npm run drug\" \"npm run build\"",
    "prepublish": "tsc && npm run build-css",
    "publish": "npm publish .",
    "build:prod": "del-cli dist &&  npm run build-css && webpack -p --config webpack.config.prod.js",
    "serve": "lite-server && concurrently \"npm run countries\" \"npm run drug\"",
    "test" : "tsc"
  },
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "@angular/upgrade": "^2.4.0",
    "@deja-js/resources": "^1.1.1",
    "bluebird": "^3.4.6",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "json-server": "^0.9.4",
    "material-design-lite": "^1.1.3",
    "moment": "^2.11.2",
    "monaco-editor": "^0.7.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "^5.1.1",
    "x2js": "^3.0.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/core-js": "^0.9.34",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.47",
    "angular2-router-loader": "^0.3.2",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "del-cli": "^0.2.1",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "lite-server": "^2.2.2",
    "node-sass": "^4.3.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "tslint": "^4.0.2",
    "typedoc": "^0.5.1",
    "typedoc-webpack-plugin": "^1.1.3",
    "typescript": "~2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-merge": "^2.4.0"
  }
}
