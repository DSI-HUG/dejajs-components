{
  "name": "json-object-mapper",
  "version": "1.0.0",
  "description": "A TypeScript library to serialize and deserialize JSON objects in a fast and non-recursive way",
  "repository": {
    "type": "git",
    "url": "https://github.com/shakilsiraj/json-object-mapper.git"
  },
  "keywords": [
    "JSON",
    "TypeScript",
    "JSON Serialize",
    "JSON Deserialize",
    "JsonProperty",
    "DTO",
    "Data Transfer Objects"
  ],
  "author": {
    "name": "Shakil Siraj",
    "email": "shakil.siraj@gmail.com"
  },
  "main": "./src/main/index.ts",
  "license": "MIT",
  "homepage": "http://github.com/shakilsiraj/json-object-mapper",
  "scripts": {
    "watch-compile": "node_modules/.bin/tsc -w -p .",
    "build": "node_modules/.bin/tsc -p .",
    "dist": "node_modules/.bin/tsc --outFile ./dist/json4typescript.js -p ./src/main",
    "test": "node_modules/.bin/jasmine-node bin --verbose --junitreport --noColor",
    "postinstall": "node_modules/.bin/typings install dt~jasmine --save-dev --global"
  },
  "devDependencies": {
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.3.4",
    "jasmine-node": "^1.14.5",
    "tslint": "^3.10.2",
    "typescript": "^2.0.10",
    "typings": "^0.8.1"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.8"
  }
}