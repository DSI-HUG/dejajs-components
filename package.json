{
    "name": "dejajs-component",
    "version": "11.0.0",
    "description": "Angular Components Library",
    "homepage": "https://github.com/DSI-HUG/dejajs-components",
    "bugs": "https://github.com/DSI-HUG/dejajs-components/issues",
    "author": "Team DSI-HUG - Hôpital Univesitaire de Genève - DSI - Domaine Patient",
    "license": "Apache-2.0",
    "keywords": [
        "angular",
        "material",
        "component"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DSI-HUG/dejajs-components.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "test:lib": "ng test @deja-js/component --watch=false",
        "test:lib:watch": "ng test @deja-js/component --watch=true",
        "lint": "eslint . --ext .js,.ts",
        "lint:fix": "eslint . --ext .js,.ts --fix",
        "e2e": "ng e2e",
        "license-check": "node scripts/licenses.js",
        "build:lib": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build @deja-js/component --configuration=production && npm run build:scss",
        "build:lib:watch": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng build @deja-js/component --watch && npm run build:scss",
        "build:scss": "scss-bundle -c component-scss-bundle.config.json",
        "postinstall": "ngcc",
        "postrelease:lib": "cpx \"./projects/deja-js/component/CHANGELOG.md\" \"./dist/deja-js/component\" && cpx \"./projects/deja-js/component/package.json\" \"./dist/deja-js/component\"",
        "release": "semantic-release --no-ci",
        "release:ci": "semantic-release && yarn run postrelease"
    },
    "dependencies": {
        "@angular/animations": "~11.0.2",
        "@angular/cdk": "~11.0.0",
        "@angular/common": "~11.0.2",
        "@angular/compiler": "~11.0.2",
        "@angular/core": "~11.0.2",
        "@angular/forms": "~11.0.2",
        "@angular/localize": "~11.0.2",
        "@angular/material": "~11.0.0",
        "@angular/material-moment-adapter": "~11.0.0",
        "@angular/platform-browser": "~11.0.2",
        "@angular/platform-browser-dynamic": "~11.0.2",
        "@angular/router": "~11.0.2",
        "angular2-text-mask": "9.0.0",
        "ckeditor": "~4.11.0",
        "core-js": "~3.6.5",
        "json-object-mapper": "~1.6.0",
        "material-icons-font": "2.1.0",
        "moment": "2.29.1",
        "monaco-editor": "0.21.2",
        "prismjs": "~1.22.0",
        "rxjs": "~6.6.3",
        "showdown": "~1.9.1",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/architect": "~0.1100.2",
        "@angular-devkit/build-angular": "~0.1100.2",
        "@angular-eslint/builder": "^0.8.0-beta.2",
        "@angular-eslint/eslint-plugin": "^0.8.0-beta.2",
        "@angular-eslint/eslint-plugin-template": "^0.8.0-beta.2",
        "@angular-eslint/template-parser": "^0.8.0-beta.2",
        "@angular/cli": "~11.0.2",
        "@angular/compiler-cli": "~11.0.2",
        "@angular/language-service": "~11.0.2",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@qiwi/multi-semantic-release": "^3.11.0",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/git": "^9.0.0",
        "@semantic-release/github": "^7.2.0",
        "@types/ckeditor": "~4.9.10",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.8",
        "@types/lodash": "~4.14.162",
        "@types/node": "~14.11.10",
        "@types/prismjs": "~1.16.1",
        "@types/showdown": "~1.9.3",
        "@typescript-eslint/eslint-plugin": "~4.8.2",
        "@typescript-eslint/parser": "~4.8.2",
        "eslint": "^7.14.0",
        "eslint-plugin-chai-friendly": "^0.6.0",
        "eslint-plugin-cypress": "^2.11.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^30.7.8",
        "eslint-plugin-no-loops": "^0.3.0",
        "eslint-plugin-prefer-arrow": "^1.2.2",
        "eslint-plugin-rxjs": "^2.1.4",
        "eslint-plugin-rxjs-angular": "^1.0.2",
        "eslint-plugin-simple-import-sort": "^5.0.3",
        "gh-pages": "~3.1.0",
        "husky": "^4.3.6",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.2.3",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "~11.0.3",
        "protractor": "~7.0.0",
        "scss-bundle": "~3.1.2",
        "semantic-release": "^17.3.0",
        "ts-node": "~9.0.0",
        "typescript": "~4.0.3"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint --edit"
        }
    }
}