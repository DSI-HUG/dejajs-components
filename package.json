    "prepublish": "npm run build-css",
    "publish": "npm publish --access public",
    "build:prod": "del-cli dist && npm run build-css && webpack -p --config webpack.config.prod.js",
    "travis-build:prod": "npm run build-css && webpack -p --config webpack.config.prod.js",
    "test": "tsc"
