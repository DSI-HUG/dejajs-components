{
    "name": "@deja-js/component",
    "version": "2.3.1",
    "description": "Angular2 Components Library",
    "homepage": "https://github.com/DSI-HUG/dejajs-components",
    "bugs": "https://github.com/DSI-HUG/dejajs-components/issues",
    "author": "Team DSI-HUG - Hôpital Univesitaire de Genève - DSI - Domaine Patient",
    "license": "Apache-2.0",
    "keywords": [
        "angular",
        "material",
        "component",
        "angular-cli"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DSI-HUG/dejajs-components.git"
    },
    "scripts": {
        "build": "ng build",
        "build:prod": "del-cli .tmp && yarn run copy:assets && ng build --prod --aot=false --env=prod --base-href https://dsi-hug.github.io/dejajs-components/",
        "build:lib": "gulp build",
        "build:lib:watch": "gulp",
        "copy:assets": "del-cli demo/vs && symlink-dir ./node_modules/monaco-editor/min/vs demo/vs",
        "docs": "yarn run docs:build",
        "docs:build": "compodoc ./src -p tsconfig.json -n @deja-js/component -d docs --hideGenerator --theme vagrant --disablePrivateOrInternalSupport --disableGraph",
        "docs:serve": "yarn run docs:build -- -s",
        "docs:watch": "yarn run docs:build -- -s -w",
        "e2e": "ng e2e",
        "licensecheck": "gulp license",
        "lint": "ng lint --type-check",
        "ng": "ng",
        "release:major": "changelog -M && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version major && git push origin",
        "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version minor && git push origin",
        "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version patch && git push origin",
        "start": "yarn run copy:assets && ng serve --port 5100",
        "test": "ng test"
    },
    "dependencies": {
        "@angular/animations": "4.2.5",
        "@angular/common": "4.2.5",
        "@angular/compiler": "4.2.5",
        "@angular/core": "4.2.5",
        "@angular/flex-layout": "2.0.0-beta.8",
        "@angular/forms": "4.2.5",
        "@angular/http": "4.2.5",
        "@angular/material": "2.0.0-beta.7",
        "@angular/platform-browser": "4.2.5",
        "@angular/platform-browser-dynamic": "4.2.5",
        "@angular/router": "4.2.5",
        "@deja-js/resources": "1.1.4",
        "@ngrx/core": "1.2.0",
        "@ngrx/store": "2.2.2",
        "angular2-text-mask": "8.0.2",
        "core-js": "2.4.1",
        "custom-event-polyfill": "0.3.0",
        "hammerjs": "2.0.8",
        "json-object-mapper": "1.4.0",
        "moment": "2.18.1",
        "monaco-editor": "0.8.3",
        "prismjs": "1.6.0",
        "reflect-metadata": "0.1.10",
        "rxjs": "5.4.1",
        "showdown": "1.7.1",
        "zone.js": "0.8.12"
    },
    "devDependencies": {
        "@angular/cli": "1.2.0",
        "@angular/compiler-cli": "4.2.5",
        "@compodoc/compodoc": "1.0.0-beta.10",
        "@deja-js/generate-changelog": "1.3.1",
        "@types/jasmine": "2.5.53",
        "@types/node": "8.0.6",
        "@types/prismjs": "1.6.2",
        "codelyzer": "3.1.1",
        "cz-conventional-changelog": "2.0.0",
        "del": "3.0.0",
        "del-cli": "1.1.0",
        "generate-changelog": "1.3.0",
        "gh-pages": "1.0.0",
        "gulp": "3.9.1",
        "gulp-json-editor": "2.2.1",
        "gulp-license-check": "1.2.1",
        "gulp-rename": "1.2.2",
        "gulp-rollup": "2.13.0",
        "is-wsl": "1.1.0",
        "jasmine-core": "2.6.4",
        "jasmine-spec-reporter": "4.1.1",
        "karma": "1.7.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-cli": "1.0.1",
        "karma-coverage-istanbul-reporter": "1.3.0",
        "karma-jasmine": "1.1.0",
        "karma-jasmine-html-reporter": "0.2.2",
        "node-sass": "4.5.3",
        "node-sass-tilde-importer": "1.0.0",
        "node-watch": "0.5.5",
        "protractor": "5.1.2",
        "rollup": "0.43.0",
        "rollup-plugin-uglify": "2.0.1",
        "run-sequence": "1.2.2",
        "symlink-dir": "1.0.3",
        "ts-node": "3.1.0",
        "tslint": "5.4.3",
        "typescript": "2.3.4"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
