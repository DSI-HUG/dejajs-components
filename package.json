{
  "name": "@deja-js/component",
  "version": "2.23.0",
  "description": "Angular2 Components Library",
  "homepage": "https://github.com/DSI-HUG/dejajs-components",
  "bugs": "https://github.com/DSI-HUG/dejajs-components/issues",
  "author": "Team DSI-HUG - Hôpital Univesitaire de Genève - DSI - Domaine Patient",
  "license": "Apache-2.0",
  "keywords": [
    "angular",
    "material",
    "component",
    "angular-cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DSI-HUG/dejajs-components.git"
  },
  "scripts": {
    "build": "ng build",
    "build:prod": "del-cli .tmp && yarn run copy:assets && ng build --prod --env=prod --base-href https://dsi-hug.github.io/dejajs-components/",
    "build:lib": "gulp build",
    "build:lib:watch": "gulp",
    "copy:assets": "del-cli demo/vs && symlink-dir ./node_modules/monaco-editor/min/vs demo/vs",
    "docs": "yarn run docs:build",
    "docs:build": "compodoc ./src -p tsconfig.json -n @deja-js/component -d docs --hideGenerator --theme vagrant --disablePrivateOrInternalSupport --disableGraph",
    "docs:serve": "yarn run docs:build -- -s",
    "docs:watch": "yarn run docs:build -- -s -w",
    "e2e": "ng e2e",
    "e2e:watch": "watch \"ng e2e\" src e2e --wait=1",
    "licensecheck": "gulp license",
    "lint": "ng lint --type-check",
    "ng": "ng",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version major && git push origin",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version minor && git push origin",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version patch && git push origin",
    "start": "yarn run copy:assets && yarn run lint && ng serve --port 5100 --base-href /",
    "test": "ng test --single-run --code-coverage",
    "test:watch": "ng test --code-coverage"
  },
  "dependencies": {
    "@angular-devkit/core": "^0.2.0",
    "@angular/animations": "5.1.3",
    "@angular/cdk": "5.0.3",
    "@angular/common": "5.1.3",
    "@angular/compiler": "5.1.3",
    "@angular/core": "5.1.3",
    "@angular/flex-layout": "2.0.0-beta.12",
    "@angular/forms": "5.1.3",
    "@angular/http": "5.1.3",
    "@angular/material": "5.0.3",
    "@angular/platform-browser": "5.1.3",
    "@angular/platform-browser-dynamic": "5.1.3",
    "@angular/router": "5.1.3",
    "@deja-js/resources": "1.2.0",
    "@ngrx/core": "1.2.0",
    "@ngrx/store": "4.1.1",
    "angular2-text-mask": "8.0.4",
    "core-js": "2.5.3",
    "css-element-queries": "^1.0.1",
    "custom-event-polyfill": "0.3.0",
    "hammerjs": "2.0.8",
    "json-object-mapper": "1.5.1",
    "moment": "2.20.1",
    "monaco-editor": "0.10.1",
    "prismjs": "1.9.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "^5.5.6",
    "showdown": "1.8.6",
    "zone.js": "0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.5.5",
    "@angular/compiler-cli": "5.1.3",
    "@compodoc/compodoc": "1.0.5",
    "@deja-js/generate-changelog": "1.4.0",
    "@types/jasmine": "2.8.3",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "4.14.92",
    "@types/node": "9.3.0",
    "@types/prismjs": "1.9.0",
    "codelyzer": "4.0.2",
    "concurrently": "3.5.1",
    "cz-conventional-changelog": "2.1.0",
    "del": "3.0.0",
    "del-cli": "1.1.0",
    "gh-pages": "1.1.0",
    "gulp": "3.9.1",
    "gulp-json-editor": "2.2.1",
    "gulp-license-check": "1.2.1",
    "gulp-rename": "1.2.2",
    "gulp-rollup": "2.15.0",
    "is-wsl": "1.1.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.3",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.5",
    "node-sass": "4.7.2",
    "node-sass-tilde-importer": "1.0.1",
    "node-watch": "0.5.5",
    "protractor": "5.2.2",
    "rollup": "0.53.3",
    "rollup-plugin-uglify": "2.0.1",
    "run-sequence": "2.2.1",
    "sonarqube-scanner": "^2.0.2",
    "symlink-dir": "1.1.0",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
