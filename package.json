{
  "name": "json-object-mapper",
  "version": "1.4.0",
  "description": "A TypeScript library to serialize and deserialize JSON objects in a fast and non-recursive way",
  "repository": {
    "type": "git",
    "url": "https://github.com/DSI-HUG/json-object-mapper.git"
  },
  "keywords": [
    "JSON",
    "TypeScript",
    "JSON Serialize",
    "JSON Deserialize",
    "JsonProperty",
    "DTO",
    "Data Transfer Objects"
  ],
  "main": "./dist/ObjectMapper.es2015.js",
  "typings": "./dist/ObjectMapper.d.ts",
  "moduleName": "ObjectMapper",
  "license": "MIT",
  "homepage": "https://github.com/DSI-HUG/json-object-mapper",
  "scripts": {
    "watch-compile": "node_modules/.bin/tsc -w -p .",
    "build": "node_modules/.bin/tsc -p .",
    "rollup": "./node_modules/.bin/rollup -c",
    "dist": "npm run build && npm run test && npm run rollup",
    "test": "node_modules/.bin/jasmine-node bin --verbose --junitreport --noColor",
    "test-karma": "node_modules/.bin/karma start ./karma.conf.js",
    "post-install": "node_modules/.bin/typings install",
    "lint": "node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version major && git push origin",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version minor && git push origin",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version patch && git push origin"
  },
  "devDependencies": {
    "@deja-js/generate-changelog": "1.3.1",
    "@types/jasmine": "^2.5.46",
    "codelyzer": "3.1.2",
    "es6-shim": "^0.35.3",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.3.4",
    "jasmine-node": "^1.14.5",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-requirejs": "^1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript": "^2.1.4",
    "process": "^0.11.9",
    "rollup": "^0.38.3",
    "rollup-plugin-typescript": "^0.8.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.10",
    "typings": "^0.8.1"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.8"
  }
}
