{
    "name": "dejajs-components-demo",
    "version": "0.0.0",
    "license": "MIT",
    "description": "Demo app for dejajs-components",
    "scripts": {
        "ng": "ng",
        "start": "ng lint && ng serve --preserve-symlinks --port 5100 --base-href /",
        "build": "del-cli .tmp && ng build --preserve-symlinks --prod --aot --build-optimizer --env=prod --base-href /",
        "test": "ng test --preserve-symlinks",
        "lint": "ng lint",
        "e2e": "ng e2e --preserve-symlinks",
        "e2e:watch": "watch \"yarn run e2e\" src e2e --wait=1",
        "unlink": "yarn unlink @deja-js/component",
        "postinstall": "yarn link @deja-js/component"
    },
    "dependencies": {
        "@angular/animations": "5.2.5",
        "@angular/common": "5.2.5",
        "@angular/compiler": "5.2.5",
        "@angular/core": "5.2.5",
        "@angular/forms": "5.2.5",
        "@angular/http": "5.2.5",
        "@angular/platform-browser": "5.2.5",
        "@angular/platform-browser-dynamic": "5.2.5",
        "@angular/platform-server": "5.2.5",
        "@angular/router": "5.2.5",
        "@angular/material": "5.2.2",
        "@angular/cdk": "5.2.2",
        "@ngrx/core": "1.2.0",
        "@ngrx/store": "4.1.1",
        "@angular/flex-layout": "5.0.0-beta.13",
        "angular2-text-mask": "8.0.4",
        "hammerjs": "2.0.8",
        "lodash": "4.17.5",
        "moment": "2.20.1",
        "monaco-editor": "0.10.1",
        "reflect-metadata": "0.1.12",
        "core-js": "2.5.3",
        "json-object-mapper": "1.5.1",
        "prismjs": "1.9.0",
        "rxjs": "5.5.6",
        "showdown": "1.8.6",
        "zone.js": "0.8.20"
    },
    "devDependencies": {
        "@angular/cli": "1.6.8",
        "@angular/compiler-cli": "5.2.5",
        "@angular/language-service": "5.2.5",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "9.3.0",
        "codelyzer": "4.0.2",
        "jasmine-core": "2.8.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "2.0.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-cli": "1.0.1",
        "karma-coverage-istanbul-reporter": "1.3.3",
        "karma-jasmine": "1.1.1",
        "karma-jasmine-html-reporter": "0.2.2",
        "karma-mocha-reporter": "2.2.5",
        "protractor": "5.2.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.7.0",
        "typescript": "~2.4.2",
        "node-sass": "4.7.2",
        "node-sass-tilde-importer": "1.0.1",
        "del-cli": "1.1.0"
    }
}
