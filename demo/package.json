        "karma-jasmine": "1.1.1",
        "karma-jasmine-html-reporter": "1.0.0",
        "karma-mocha-reporter": "2.2.5",
        "protractor": "5.3.0",
        "ts-node": "5.0.1",
        "tslint": "5.9.1",
        "typescript": "2.7.2",
