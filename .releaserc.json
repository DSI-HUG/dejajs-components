{
    "branches": [
        "master",
        "develop",
        {
            "name": "develop",
            "prerelease": true
        }
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "projects/deja-js/component/CHANGELOG.md"
            }
        ],
        [
            "@semantic-release/npm",
            {
                "npmPublish": false,
                "pkgRoot": "projects/deja-js/component/"
            }
        ],
        "@semantic-release/git",
        {
            "assets": ["projects/deja-js/component/package.json", "projects/deja-js/component/CHANGELOG.md"],
            "message": "chore(dejajs-component): ${nextRelease.version}"
        }
    ],
    "repositoryUrl": "git@github.com:vapkse/deja-js-component.git"
}