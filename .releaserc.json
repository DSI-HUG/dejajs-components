{
    "branches": [
        "master",
        {
            "name": "develop",
            "prerelease": true
        }
    ],
    "repositoryUrl": "ssh://git@vmlp-00012.hcuge.ch:7999/deja/dejajs-dpi.git",
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "projects/dejajs-dpi/CHANGELOG.md"
            }
        ],
        [
            "@semantic-release/npm",
            {
                "npmPublish": false,
                "pkgRoot": "projects/dejajs-dpi"
            }
        ],
        [
            "@semantic-release/git",
            {
                "assets": ["projects/dejajs-dpi/package.json", "projects/dejajs-dpi/CHANGELOG.md"],
                "message": "chore(release): ${nextRelease.version}"
            }
        ]
    ]
}
